    },
    "devDependencies": {
        "@babel/cli": "^7.23.4",
        "@babel/core": "^7.16.5",
        "@babel/preset-env": "^7.23.8",
        "@babel/preset-react": "^7.23.3",
        "autoprefixer": "^10.4.16",
        "babel-loader": "^9.1.3",
        "postcss": "^8.4.33",
        "postcss-cli": "^11.0.0",
        "react": "^17.0.2",
